
dumpfile="\
f emptyfile
f textfile
> hello
> world
d dir
d dir/dir
d dir/dir/dir
d dir/dir/dir/dir
f dir/dir/dir/dir/file
> foo
d emptydir
"

textfile="\
hello
world
"

subdirfile="\
foo
"

arrange_dumpfile() {
  echo -n "$dumpfile" > dumpfile
}

arrange_dirs() {
  mkdir emptydir
  touch emptyfile
  echo -n "$textfile" > textfile
  mkdir -p dir/dir/dir/dir
  echo -n "$subdirfile" > dir/dir/dir/dir/file
}
